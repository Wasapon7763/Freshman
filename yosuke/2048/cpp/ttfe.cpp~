#include <iostream>
using namespace std;
void disp(int board[4][4]){
  for (int i=0;i<4;i++){
    cout<<"                    ";
    for (int j=0;j<4;j++){
      cout<<board[i][j]<<"    ";
    }
    cout<<endl<<endl;
  }
}
int main(){
  char key;
  int board[4][4]={0};
  while (1){
    cin>>key;
    switch (key){
    case 'a':
      //left();
      //break;
    case 'd':
      //right();
      break;
    case 'w':
      //up();
      break;
    case 's':
      //down();
      break;
    }
    disp(board);
  }
}
