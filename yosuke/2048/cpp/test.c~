#include<stdio.h>
#include<conio.h>
#include<windows.h>
int main(void){
  int c;
  c = getch();
  switch((c == 0x00 || c == 0xe0) ? getch(): c) {
  case 0x1b: // ESC
    break;
  case 0x08: // BS
    break;
  case 0x09: // TAB
    break;
  case 0x03: // CTRL-C
    break;
  case 0x0d: // ENTER
    break;
  case 0x20: // SPC
    break;
  case 0x48: // ↑
    printf("上");
    break;
  case 0x4b: // ←
    printf("左");
    break;
  case 0x4d: // →
    printf("右");
    break;
  case 0x50: // ↓
    printf("下");
    break;
  case 0x52: //INS
    break;
  case 0x53: //DEL
    break;
  case 0x47: // HOME
    break;
  case 0x4f: // END
    break;
  case 0x49: // PAGEUP
    break;
  case 0x51: // PAGEDOWN
    break;
  case 0x3b: // F1
    break;
  case 0x3c: // F2
    break;
  case 0x3d: // F3
    break;
  case 0x3e: // F4
    break;
  case 0x3f: // F5
    break;
  case 0x40: // F6
    break;
  case 0x41: // F7
    break;
  case 0x42: // F8
    break;
  case 0x43: // F9
    break;
  case 0x44: // F10
    return;
  case 0x85: // F11
    break;
  case 0x86: // F12
    break;
  default:
    break;
  }
}
